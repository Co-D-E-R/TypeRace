{
    "version": 2,
    "builds": [
      {
        "src": "fronted/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "fronted/dist",
          "buildCommand": "npm run build"
        }
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/fronted/dist/$1"
      }
    ]
  }